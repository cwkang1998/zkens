{
  "name": "zk-ens-e2e",
  "version": "1.0.0",
  "private": true,
  "description": "End-to-end tests for zk-ENS demo protocol",
  "type": "module",
  "scripts": {
    "up": "docker compose up -d --build chain backend",
    "wait": "tsx scripts/wait-stack.ts",
    "test": "vitest run --passWithNoTests",
    "down": "docker compose down -v",
    "e2e": "npm run up && npm run wait && npm run test && npm run down"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "tsx": "^4.19.2",
    "vitest": "^2.1.3",
    "viem": "^2.37.5"
  }
}
